local ButtonExample = Tab8:CreateButton({
    Name = "自动挥剑",
    Callback = function()
        print("按钮被点击了!")
        while true do
local args = {
	"swingKatana"
}
game:GetService("Players").LocalPlayer:WaitForChild("ninjaEvent"):FireServer(unpack(args))
wait(0.1)
end
    end,
})