    end
})

-- 4. 滑块(Slider)示例
local SliderExample = Tab2:CreateSlider({
    Name = "speed",
    Range = {0, 1000},
    Increment = 1,
    CurrentValue = 16,
    Flag = "SliderExample",
    Callback = function(Value)
        print("滑块值:", Value)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
    end,
})


