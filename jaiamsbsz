function Notify(Title1, Text1, Icon1, Time1)
  game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = Title1,
    Text = Text1,
    Icon = lcon1,
    Duration = Time1,
  })
end
Notify("猫帝脚本", "祝你玩的愉快", "rbxassetid://89234940088127", 3)
Notify("禁止倒卖", "倒卖死全家", "rbxassetid://89234940088127", 3)
print("猫帝帮你开启了反挂机")
		local vu = game:GetService("VirtualUser")
		game:GetService("Players").LocalPlayer.Idled:connect(function()
		   vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
		   wait(1)
		   vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
		end)
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()


local Window = Rayfield:CreateWindow({
    Name = "猫帝 脚本",
    LoadingTitle = "加载中...",
    LoadingSubtitle = "欢迎使用 猫帝脚本",
    ConfigurationSaving = {
        Enabled = false,
    },
})


local Tab1 = Window:CreateTab("关于")
local Tab2 = Window:CreateTab("通用")
